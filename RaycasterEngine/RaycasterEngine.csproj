<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PublishAot>true</PublishAot>
    <InvariantGlobalization>true</InvariantGlobalization>
    <PackageId>RaycasterEngine</PackageId>
    <Title>Lightweight Raycaster Engine</Title>
    <Version>1.0.0</Version>
    <Authors>Gooberheads</Authors>
    <Company>A Goober's Studio</Company>
    <Product>RaycasterEngine</Product>
    <Description>A simple lightweight Raycasting Engine that can generate random maps.</Description>
    <Copyright>@ 2025 Gooberheads</Copyright>
    <PackageProjectUrl>https://github.com/GooberHeads6297/LightweightRaycasterEngine</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GooberHeads6297/LightweightRaycasterEngine</RepositoryUrl>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

</Project>
